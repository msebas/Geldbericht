<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" prefHeight="636.0" prefWidth="967.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.mcservice.geldbericht.PrimaryController">
   <children>
      <MenuBar VBox.vgrow="NEVER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#startCompanyManager" text="Betriebsmanager" />
              <MenuItem mnemonicParsing="false" onAction="#startVatTypeManager" text="Steuermanager" />
              <MenuItem mnemonicParsing="false" onAction="#startAccountManager" text="Kontenmanager" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <TabPane prefHeight="576.0" prefWidth="748.0" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
        <tabs>
          <Tab text="Buchungen">
            <content>
                   <fx:include fx:id="transactionTab" source="TransactionInputPane.fxml"/>
            </content>
          </Tab>
          <Tab text="PDF Erstellen">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <VBox prefHeight="324.0" prefWidth="748.0">
                           <children>
						      <Label text="Bitte Monat auswÃ¤hlen" />
						      <ChoiceBox fx:id="pdfCompanySelector" onAction="#pdfCompanyChanged" prefWidth="150.0" />
                              <ChoiceBox fx:id="pdfAccountSelector" onAction="#pdfAccountChanged" prefWidth="150.0" />
                              <ComboBox fx:id="pdfMonthSelector" onAction="#pdfMonthChanged" />
						      <Button fx:id="pdfCreateButton" onAction="#createPDF" text="PDF Bericht erstellen" />
						    </children>
                        </VBox>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
      <HBox prefHeight="0.0" prefWidth="967.0">
         <children>
            <Label minHeight="-Infinity" minWidth="-Infinity" text="Status Rechts">
               <HBox.margin>
                  <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
               </HBox.margin></Label>
            <Pane prefHeight="0.0" prefWidth="903.0" HBox.hgrow="ALWAYS" />
            <Label minHeight="-Infinity" minWidth="-Infinity" text="Status Links">
               <HBox.margin>
                  <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
               </HBox.margin></Label>
         </children>
      </HBox>
   </children>
</VBox>
